# Docker image with jdk, lein, make
# terraform, aws-cli
# See https://github.com/techops-recruiting-systems/docker-infra-interview-dojo
DOJO_DOCKER_IMAGE="joii/infra-joi-dojo:0.3.0"

# Mount cached build deps to make .jar builds faster in the dojo container
DOJO_DOCKER_OPTIONS="-v $HOME/.m2:/home/dojo/.m2"
